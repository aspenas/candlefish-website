{
  "name": "@clos/nanda-orchestrator",
  "version": "1.0.0",
  "description": "NANDA Autonomous Service Orchestration System",
  "main": "orchestrator.js",
  "scripts": {
    "start": "tsx orchestrator.ts",
    "dev": "tsx watch orchestrator.ts",
    "build": "tsc",
    "start:all": "npm-run-all --parallel start:orchestrator start:discovery start:optimizer start:guardian start:healer",
    "start:orchestrator": "NANDA_PORT=5100 tsx orchestrator.ts",
    "start:discovery": "NANDA_PORT=5101 AGENT_TYPE=discovery tsx agents/discovery.ts",
    "start:optimizer": "NANDA_PORT=5102 AGENT_TYPE=optimizer tsx agents/optimizer.ts",
    "start:guardian": "NANDA_PORT=5103 AGENT_TYPE=guardian tsx agents/guardian.ts",
    "start:healer": "NANDA_PORT=5104 AGENT_TYPE=healer tsx agents/healer.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "socket.io": "^4.6.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/js-yaml": "^4.0.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "npm-run-all": "^4.1.5"
  }
}