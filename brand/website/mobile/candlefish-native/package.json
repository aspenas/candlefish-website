{
  "name": "candlefish-native",
  "version": "1.0.0",
  "private": true,
  "description": "Native mobile implementations of the bioluminescent fish animation",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start:web",
    "start": "react-native start",
    "test": "jest",
    "test:detox": "detox test",
    "lint": "eslint .",
    "clean": "react-native clean-project",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace CandlefishNative.xcworkspace -scheme CandlefishNative -configuration Release",
    "codegen": "react-native codegen",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-reanimated": "^3.8.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-canvas": "^0.1.38",
    "react-native-svg": "^14.1.0",
    "react-native-skia": "^0.1.219",
    "react-native-sensors": "^7.3.6",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-device-info": "^10.13.1",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-vision-camera": "^3.8.2",
    "react-native-background-timer": "^2.4.1",
    "react-native-keychain": "^8.1.3",
    "react-native-share": "^10.0.2",
    "react-native-vector-icons": "^10.0.3",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/stack": "^6.3.25",
    "zustand": "^4.5.1",
    "expo": "~50.0.14",
    "expo-gl": "~13.6.0",
    "expo-gl-cpp": "~13.6.0",
    "expo-sensors": "~12.9.1",
    "expo-haptics": "~12.8.1",
    "expo-device": "~5.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.5",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "detox": "^20.18.5",
    "patch-package": "^8.0.0",
    "@storybook/react-native": "^6.5.6"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/candlefish-ai/brand"
  },
  "keywords": [
    "react-native",
    "animation",
    "bioluminescent",
    "fish",
    "mobile",
    "native"
  ],
  "author": "Candlefish AI",
  "license": "MIT"
}