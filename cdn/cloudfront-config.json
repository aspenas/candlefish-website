        "ContentTypeOptions": {
          "Override": false
        },
        "StrictTransportSecurity": {
          "Override": false,
          "IncludeSubdomains": true,
          "Preload": true,
          "AccessControlMaxAgeSec": 63072000
        },
        "ContentSecurityPolicy": {
          "Override": false,
          "ContentSecurityPolicy": "default-src 'self' https://api.candlefish.ai; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.jsdelivr.net; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https://api.candlefish.ai wss://ws.candlefish.ai; frame-ancestors 'none'; base-uri 'self'; form-action 'self'"
        }
      },
      "CustomHeadersConfig": {
        "Quantity": 3,
        "Items": [
          {
            "Header": "X-Content-Type-Options",
            "Value": "nosniff",
            "Override": false
          },
          {
            "Header": "X-Frame-Options",
            "Value": "DENY",
            "Override": false
          },
          {
            "Header": "Permissions-Policy",
            "Value": "camera=(), microphone=(), geolocation=(), interest-cohort=()",
            "Override": false
          }
        ]
      },
      "ServerTimingHeadersConfig": {
        "Enabled": true,
        "SamplingRate": 0.001
