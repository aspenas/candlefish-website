  "Comment": "Security Dashboard CDN - Optimized for Performance",
  "Logging": {
    "Enabled": true,
    "IncludeCookies": true,
    "Bucket": "security-dashboard-cdn-logs.s3.amazonaws.com",
    "Prefix": "cloudfront/"
  },
  "PriceClass": "PriceClass_All",
  "Enabled": true,
  "ViewerCertificate": {
