      {
        "PathPattern": "*.js",
        "TargetOriginId": "security-dashboard-s3-static",
        "ViewerProtocolPolicy": "https-only",
        "Compress": true,
        "ForwardedValues": {
          "QueryString": true,
          "Cookies": {
            "Forward": "none"
          }
        },
        "MinTTL": 86400,
        "DefaultTTL": 604800,
        "MaxTTL": 31536000
