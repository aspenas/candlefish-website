    "FieldLevelEncryptionId": "",
    "ForwardedValues": {
      "QueryString": true,
      "Cookies": {
        "Forward": "whitelist",
        "WhitelistedNames": {
          "Quantity": 3,
          "Items": ["session", "auth-token", "csrf-token"]
        }
      },
      "Headers": {
        "Quantity": 6,
        "Items": [
          "Accept",
          "Accept-Language",
          "Authorization",
          "CloudFront-Viewer-Country",
          "Origin",
          "Referer"
