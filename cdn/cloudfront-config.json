    "Quantity": 3,
    "Items": [
      {
        "Id": "security-dashboard-alb",
        "DomainName": "alb.security.candlefish.ai",
        "CustomOriginConfig": {
          "HTTPPort": 80,
          "HTTPSPort": 443,
          "OriginProtocolPolicy": "https-only",
          "OriginSslProtocols": {
            "Quantity": 3,
            "Items": ["TLSv1.2", "TLSv1.3"]
          },
          "OriginReadTimeout": 30,
          "OriginKeepaliveTimeout": 5
        },
        "ConnectionAttempts": 3,
        "ConnectionTimeout": 10,
        "OriginShield": {
          "Enabled": true,
          "OriginShieldRegion": "us-east-1"
        }
      },
      {
        "Id": "security-dashboard-s3-static",
        "DomainName": "security-dashboard-static.s3.amazonaws.com",
        "S3OriginConfig": {
          "OriginAccessIdentity": "origin-access-identity/cloudfront/E127EXAMPLE51Z"
        }
      },
      {
        "Id": "security-dashboard-api",
        "DomainName": "api.security.candlefish.ai",
        "CustomOriginConfig": {
          "HTTPPort": 80,
          "HTTPSPort": 443,
          "OriginProtocolPolicy": "https-only",
          "OriginSslProtocols": {
            "Quantity": 2,
            "Items": ["TLSv1.2", "TLSv1.3"]
          },
          "OriginReadTimeout": 60,
          "OriginKeepaliveTimeout": 5
        }
      }
    ]
  },
  "OriginGroups": {
    "Quantity": 1,
    "Items": [
      {
        "Id": "security-dashboard-failover-group",
        "FailoverCriteria": {
          "StatusCodes": {
            "Quantity": 5,
            "Items": [500, 502, 503, 504, 404]
          }
        },
        "Members": {
          "Quantity": 2,
          "Items": [
            {
              "OriginId": "security-dashboard-alb"
            },
            {
              "OriginId": "security-dashboard-s3-static"
            }
          ]
        }
      }
    ]
  },
  "DefaultCacheBehavior": {
