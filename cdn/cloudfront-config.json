          "EventType": "viewer-request",
          "IncludeBody": false
        },
        {
          "LambdaFunctionARN": "arn:aws:lambda:us-east-1:681214184463:function:security-dashboard-edge-headers:1",
          "EventType": "origin-response",
          "IncludeBody": false
        }
      ]
    },
    "FunctionAssociations": {
      "Quantity": 0
