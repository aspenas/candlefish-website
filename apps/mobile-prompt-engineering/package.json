{
  "name": "mobile-prompt-engineering",
  "version": "1.0.0",
  "description": "React Native mobile companion app for Candlefish AI prompt engineering system",
  "main": "App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build-web": "expo export:web",
    "clean": "rm -rf node_modules && npm install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/drawer": "^6.6.6",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "expo": "~49.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-splash-screen": "~0.20.5",
    "expo-local-authentication": "~13.4.1",
    "expo-secure-store": "~12.3.1",
    "expo-notifications": "~0.20.1",
    "expo-camera": "~13.4.4",
    "expo-document-picker": "~11.5.4",
    "expo-clipboard": "~4.3.1",
    "expo-haptics": "~12.4.0",
    "expo-sharing": "~11.5.0",
    "expo-linking": "~5.0.2",
    "expo-av": "~13.4.1",
    "expo-speech": "~11.3.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-modal": "^13.0.1",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-flash-message": "^0.4.2",
    "react-native-paper": "^5.10.6",
    "react-native-elements": "^3.4.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "13.9.0",
    "react-native-keychain": "^8.1.3",
    "react-native-background-job": "^0.2.7",
    "react-native-async-storage": "@react-native-async-storage/async-storage@^1.19.3",
    "react-hook-form": "^7.46.2",
    "react-query": "^3.39.3",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "react-native-markdown-display": "^7.0.2",
    "react-native-code-push": "^8.2.1",
    "react-native-device-info": "^10.11.0",
    "react-native-netinfo": "@react-native-community/netinfo@^9.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.5",
    "@types/lodash": "^4.14.199",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@types/react": "~18.2.14"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  }
}