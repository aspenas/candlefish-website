{
  "name": "@candlefish-ai/collaboration-mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Real-time collaborative document editor - React Native mobile app",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace CollaborationMobile.xcworkspace -scheme CollaborationMobile -configuration Release archive",
    "clean": "react-native clean && cd ios && xcodebuild clean && cd ../android && ./gradlew clean",
    "pods": "cd ios && pod install"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@react-native-async-storage/async-storage": "^1.19.4",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "apollo-link-retry": "^2.2.16",
    "apollo3-cache-persist": "^0.14.1",
    "date-fns": "^2.30.0",
    "flipper-plugin-apollo-devtools": "^0.0.2",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-background-sync": "^1.0.5",
    "react-native-biometrics": "^3.0.1",
    "react-native-blob-util": "^0.19.4",
    "react-native-device-info": "^10.11.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-file-access": "^3.0.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-keychain": "^8.1.3",
    "react-native-mmkv": "^2.10.2",
    "react-native-pager-view": "^6.2.3",
    "react-native-permissions": "^4.0.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
