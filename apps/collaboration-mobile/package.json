    "@testing-library/react-native": "^12.4.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.7",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "flipper": "^0.212.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}