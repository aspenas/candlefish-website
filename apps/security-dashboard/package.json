{
  "name": "@candlefish/security-dashboard",
  "version": "1.0.0",
  "type": "module",
  "description": "Enterprise Security Dashboard with real-time monitoring and JWT authentication",
  "keywords": [
    "security",
    "dashboard",
    "monitoring",
    "react",
    "tailwind",
    "websocket"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:unit:frontend": "vitest run src/**/*.test.{ts,tsx}",
    "test:unit:backend": "vitest run src/graphql/**/*.test.ts",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "test:integration:api": "vitest run tests/integration/api/**/*.test.ts",
    "test:integration:database": "vitest run tests/integration/database/**/*.test.ts",
    "test:integration:websocket": "vitest run tests/integration/websocket/**/*.test.ts",
    "test:integration:external": "vitest run tests/integration/external/**/*.test.ts",
    "test:e2e": "npm run test:e2e:cypress && npm run test:e2e:playwright",
    "test:e2e:cypress": "cypress run",
    "test:e2e:cypress:open": "cypress open",
    "test:e2e:cypress:headless": "cypress run --headless",
    "test:e2e:playwright": "playwright test",
    "test:e2e:playwright:headed": "playwright test --headed",
    "test:visual": "playwright test --config=playwright.config.visual.ts",
    "test:accessibility": "playwright test tests/accessibility/",
    "test:performance": "k6 run tests/performance/k6/security-dashboard-load-test.js",
    "test:security": "k6 run tests/security/penetration-tests.js",
    "test:all": "node scripts/test-runner.js",
    "test:mobile": "cd mobile && npm test",
    "coverage:merge": "nyc merge coverage/frontend coverage/backend coverage/merged",
    "coverage:report": "nyc report --reporter=html --reporter=text --reporter=lcov",
    "quality-gate:check": "node scripts/test-runner.js --gates",
    "security:scan": "npm audit && npm run test:security",
    "security:audit": "npm audit --audit-level high",
    "performance:test": "npm run test:performance",
    "performance:report": "node scripts/generate-performance-report.js",
    "performance:analyze": "node scripts/analyze-performance-metrics.js",
    "performance:compare": "node scripts/compare-performance-baseline.js",
    "accessibility:test": "npm run test:accessibility",
    "accessibility:report": "node scripts/generate-accessibility-report.js",
    "badges:update": "node scripts/update-badges.js",
    "reports:generate": "node scripts/test-runner.js --reports",
    "wait-for-services": "wait-on http://localhost:3005 http://localhost:4000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "license-check": "license-checker --summary"
  },
  "dependencies": {
    "@apollo/client": "^4.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^1.7.17",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "7.0.0-beta.16",
    "@mui/material": "^7.3.1",
    "@mui/x-charts": "^8.10.2",
    "@mui/x-data-grid": "^8.10.2",
    "@mui/x-date-pickers": "^8.10.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "clsx": "^2.0.0",
    "d3": "^7.9.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^11.0.0",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.1.2",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.20.1",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "subscriptions-transport-ws": "^0.11.0",
    "tailwind-merge": "^2.0.0",
    "ws": "^8.18.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-window": "^1.8.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^1.0.0",
    "cypress": "^13.0.0",
    "@playwright/test": "^1.40.0",
    "axe-core": "^4.8.0",
    "@axe-core/playwright": "^4.8.0",
    "nyc": "^15.1.0",
    "wait-on": "^7.0.0",
    "chalk": "^4.1.2",
    "license-checker": "^25.0.1",
    "prettier": "^3.0.0"
  }
}
